  "author": {
    "name": "Christopher Diggins",
    "email": "cdiggins@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdiggins/myna-parser.git"
  },
  "devDependencies": {
    "istanbul": "~0.4.5", 
    "mocha": "~3.2.0",
    "typedoc": "~0.5.3",
    "typescript": "~2.0.10"
  }
}
