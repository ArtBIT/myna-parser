{
  "name": "myna-parser",
  "version": "0.1.0",
  "description": "A JavaScript text parsing library.",
  "main": "Myna",
  "scripts": {
    "test": "mocha tests/mocha_runner.js",
    "build": "tsc",
    "win-cover": "istanbul cover %AppData%/npm/node_modules/mocha/bin/_mocha tests/mocha_runner.js -R spec",
